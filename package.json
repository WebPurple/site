{
  "name": "web-purple",
  "version": "1.0.0",
  "description": "Web Purple Community Web Site",
  "main": "src/boot.jsx",
  "scripts": {
    "start": "node src/server/server.js",
    "build": "webpack",
    "build:prod": "cross-env NODE_ENV=production webpack",
    "build:watch": "webpack --watch",
    "lint": "./node_modules/.bin/eslint src/** test/**",
    "test": "mocha --require babel-register --recursive",
    "test:cov": "babel-node node_modules/isparta/bin/isparta cover --report lcovonly --report html node_modules/mocha/bin/_mocha -- --recursive",
    "precoveralls": "npm run test:cov",
    "coveralls": "coveralls < coverage/lcov.info",
    "clean": "rimraf node_modules coverage public/build",
    "postinstall": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "https://git.epam.com/ilya_kuznetsov/web-purple.git"
  },
  "author": "Nikita_Kirsanov@epam.com, Ilya_Kuznetsov@epam.com",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.15.0",
    "cheerio": "^0.20.0",
    "connect-history-api-fallback": "^1.2.0",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "isomorphic-fetch": "^2.2.1",
    "material-ui": "^0.14.4",
    "mongoose": "^4.4.7",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.0",
    "passport-vkontakte": "^0.4.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.1",
    "react-tap-event-plugin": "^0.2.2",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1",
    "webpack": "^1.12.14"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "coveralls": "^2.11.12",
    "cross-env": "^2.0.0",
    "eslint": "^3.3.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.0",
    "force-case-sensitivity-webpack-plugin": "^0.1.1",
    "html-webpack-plugin": "^2.22.0",
    "imports-loader": "^0.6.5",
    "isparta": "^4.0.0",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.2.0",
    "sinon": "^1.17.3"
  }
